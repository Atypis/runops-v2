{
  "name": "aef-test-harness",
  "version": "1.0.0",
  "type": "module",
  "description": "Rapid iteration test environment for AEF",
  "scripts": {
    "start": "node server.js",
    "start:unified": "node workflows/interfaces/frontend-adapter.js",
    "start:v2": "node workflows/interfaces/frontend-adapter-v2.js",
    "dev": "node --watch server.js",
    "test:node": "node --watch test-node.js",
    "test:primitive": "node --watch test-primitive.js",
    "test:workflow": "node --watch test-workflow.js",
    "test:problem": "node --watch test-workflow.js --problem",
    "workflow": "node workflows/interfaces/cli.js",
    "workflow:list": "node workflows/interfaces/cli.js list",
    "workflow:server": "node workflows/interfaces/server.js"
  },
  "dependencies": {
    "@browserbasehq/stagehand": "latest",
    "playwright": "latest",
    "openai": "latest",
    "dotenv": "latest",
    "express": "latest",
    "cors": "latest",
    "zod": "latest",
    "commander": "latest"
  }
}