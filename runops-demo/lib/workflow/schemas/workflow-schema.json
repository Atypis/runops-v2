{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "meta": {
      "type": "object",
      "properties": {
        "id": { "type": "string" },
        "title": { "type": "string" },
        "version": { "type": "string" }
      },
      "required": ["id"]
    },
    "public": {
      "type": "object",
      "properties": {
        "nodes": {
          "type": "array",
          "items": { "type": "object" }
        }
      },
      "required": ["nodes"]
    }
  },
  "required": ["meta", "public"]
}

